{
  "compilerOptions": {
      "outDir": "./dist/",
      "strictNullChecks": true,
      "noUnusedLocals" : true,
      "noImplicitThis": true,
      "alwaysStrict": true,
      "declaration": true,
      "sourceMap": true,
      "target": "esnext",
      "module": "commonJS",
      "baseUrl": ".",
      "types": ["node"],
      "paths": {
          "*": ["./typings/*"]
      }
  },
    "exclude": [
      "node_modules",
      "dist"
    ]
}
